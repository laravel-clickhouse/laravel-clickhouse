{
    "name": "laravel-clickhouse/laravel-clickhouse",
    "description": "A ClickHouse based Eloquent model and Query builder for Laravel",
    "keywords": [
        "php",
        "laravel",
        "eloquent",
        "clickhouse",
        "database",
        "model"
    ],
    "homepage": "https://github.com/laravel-clickhouse/laravel-clickhouse",
    "authors": [
        { "name": "Sasaya", "email": "sasaya@percussion.life" }
    ],
    "license": "MIT",
    "require": {
        "php": "^8.2",
        "smi2/phpclickhouse": "^1.5"
    },
    "require-dev": {
        "phpunit/phpunit": "^11.3",
        "mockery/mockery": "^1.6",
        "phpstan/phpstan": "^1.12",
        "laravel/pint": "^1.17",
        "illuminate/database": "^11.21",
        "laravel/serializable-closure": "^1.3"
    },
    "autoload": {
        "psr-4": {
            "ClickHouse\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ClickHouse\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "ClickHouse\\Laravel\\ClickHouseServiceProvider"
            ]
        }
    },
    "scripts": {
      "test": "phpunit --colors",
      "phpstan": "phpstan analyse",
      "cs": "pint src tests --test",
      "cs:fix": "pint src tests"
    }
}
